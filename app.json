{
  "expo": {
    "name": "SmartTrip AI",
    "slug": "traveling",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoicon2.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/lg8.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "EXPO_GOOGLE_MAP_KEY": "your-google-api-key-here",
      "RAPIDAPI_KEY": "your-rapidapi-key-here",
      "eas": {
        "projectId": "20cee84b-97d3-47a4-ad65-8e8d8ba7b8c8"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoicon2.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yash_gahlot.AITravelPlannerApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "yash-gahlot"
  }
}
